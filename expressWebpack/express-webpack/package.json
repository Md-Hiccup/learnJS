{
  "name": "express-webpack",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-scripts": "1.0.14"
  },
  "scripts": {
    "dev": "./node_modules/.bin/webpack-dev-server -d --watch",
